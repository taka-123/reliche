{
  "name": "laravel-nuxt-template-frontend",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "lint": "eslint --ext .js,.ts,.vue .",
    "lint:fix": "eslint --ext .js,.ts,.vue . --fix",
    "lint:css": "stylelint '**/*.{css,scss,vue}'",
    "lint:css:fix": "stylelint '**/*.{css,scss,vue}' --fix",
    "test": "vitest",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.vue": [
      "stylelint --fix"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@mdi/font": "^7.3.67",
    "@pinia/nuxt": "^0.5.1",
    "axios": "^1.6.2",
    "nuxt": "3.16.1",
    "pinia": "^3.0.1",
    "sass": "^1.86.0",
    "vite-plugin-vuetify": "^2.1.0",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5",
    "vuetify": "^3.4.3"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@pinia/testing": "^1.0.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vitest/coverage-v8": "^3.2.4",
    "@vue/test-utils": "^2.4.6",
    "@vueuse/core": "^10.6.1",
    "@vueuse/nuxt": "^10.6.1",
    "esbuild": "0.25.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "^9.18.1",
    "happy-dom": "^17.4.4",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "stylelint": "^16.21.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  },
  "version": "1.0.0",
  "description": "Laravel + Nuxt + PostgreSQL テンプレートのフロントエンド部分",
  "main": ".eslintrc.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
